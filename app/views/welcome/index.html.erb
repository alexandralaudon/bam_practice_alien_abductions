<h1>List of Aliens</h1>


<% @aliens.each do |alien| %>
    <%= link_to "#{alien.species} from the planet #{alien.planet}", alien_path(alien) %>
<% end %>

<%= link_to "New Alien", new_alien_path %>

<h1>List of People</h1>

<% @people.each do |person| %>
  <%= link_to "#{person.full_name} - #{person.occupation}", person_path(person) %>
<% end %>

<%= link_to "New Person", new_person_path %>
